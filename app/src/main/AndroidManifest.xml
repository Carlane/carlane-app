<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cherry.alok.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LocationActivityMap"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UserCarsActivty"
            android:label="@string/title_activity_user_cars_activty"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cherry.alok.myapplication.MainActivity" />
        </activity>
        <activity
            android:name=".PagerActivity"
            android:theme="@style/MyTheme.Detail" />
        <activity
            android:name=".UserCar_CollapseHeader"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_Services"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_AddCar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SelectSlotActivity" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".OrderReview"></activity>
    </application>

</manifest>